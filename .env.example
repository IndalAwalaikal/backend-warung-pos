# Backend environment variables (example)
# Copy this to .env and fill sensitive values for your environment

# Database connection
DB_USER=root
DB_PASS=
DB_HOST=127.0.0.1
DB_PORT=3306
DB_NAME=warung_pos

# JWT
JWT_SECRET=replace_with_a_strong_random_secret

# Server
PORT=8085

# Optional: initial admin seeder values (not used by default)
ADMIN_EMAIL=admin@warung.local
ADMIN_PASSWORD=changeme

# Frontend origin (used for CORS)
# Set this to your frontend URL, e.g. http://localhost:5173
FRONTEND_ORIGIN=http://localhost:5173
